<?xml version="1.0" encoding="UTF-8"?>

<DataModel packageName="com.webratio.webapp" volatileKeyPolicy="" useQualifiedEntities="">
  <!-- DATABASES -->
  <Database id="application_vdb" url="jdbc:h2:mem:application_vdb" schema="" duration="volatileApplication"/>
  <Database id="session_vdb" url="jdbc:h2:mem:session_vdb" schema="" duration="volatileSession"/>
  <!-- ENTITIES -->
  <Entity id="User" name="User" className="com.webratio.webapp.User" duration="persistent">
    <Attribute id="userOID" key="true" name="oid" fieldName="oid" type="integer" javaType="java.lang.Integer"/>
    <Attribute id="userName" key="false" name="userName" fieldName="userName" type="string" javaType="java.lang.String"/>
    <Attribute id="password" key="false" name="password" fieldName="password" type="password" javaType="java.lang.String"/>
    <Attribute id="email" key="false" name="email" fieldName="email" type="string" javaType="java.lang.String"/>
    <OutgoingRoles>User2Group|User2DefaultGroup</OutgoingRoles>
    <InverseRoles>User2DefaultGroup</InverseRoles>
  </Entity>
  <Entity id="Group" name="Group" className="com.webratio.webapp.Group" duration="persistent">
    <Attribute id="groupOID" key="true" name="oid" fieldName="oid" type="integer" javaType="java.lang.Integer"/>
    <Attribute id="groupName" key="false" name="groupName" fieldName="groupName" type="string" javaType="java.lang.String"/>
    <OutgoingRoles>Group2User|DefaultGroup2User|Group2DefaultModule|Group2Module</OutgoingRoles>
    <InverseRoles>Group2User|DefaultGroup2User|Group2DefaultModule</InverseRoles>
  </Entity>
  <Entity id="Module" name="Module" className="com.webratio.webapp.Module" duration="persistent">
    <Attribute id="moduleOID" key="true" name="oid" fieldName="oid" type="integer" javaType="java.lang.Integer"/>
    <Attribute id="moduleID" key="false" name="moduleID" fieldName="moduleID" type="string" javaType="java.lang.String"/>
    <Attribute id="moduleName" key="false" name="moduleName" fieldName="moduleName" type="string" javaType="java.lang.String"/>
    <OutgoingRoles>DefaultModule2Group|Module2Group</OutgoingRoles>
    <InverseRoles>DefaultModule2Group|Module2Group</InverseRoles>
  </Entity>
  <!-- RELATIONSHIP_ROLES -->
  <Role id="User2Group" inverseId="Group2User" source="User" target="Group" maxCard="many" fieldName="groups" derived="false" direct="true"/>
  <Role id="Group2User" inverseId="User2Group" source="Group" target="User" maxCard="many" fieldName="users" derived="false" direct="false"/>
  <Role id="User2DefaultGroup" inverseId="DefaultGroup2User" source="User" target="Group" maxCard="1" fieldName="defaultGroup" derived="false" direct="false"/>
  <Role id="DefaultGroup2User" inverseId="User2DefaultGroup" source="Group" target="User" maxCard="many" fieldName="defaultUsers" derived="false" direct="false"/>
  <Role id="Group2DefaultModule" inverseId="DefaultModule2Group" source="Group" target="Module" maxCard="1" fieldName="defaultModule" derived="false" direct="false"/>
  <Role id="DefaultModule2Group" inverseId="Group2DefaultModule" source="Module" target="Group" maxCard="many" fieldName="defaultGroups" derived="false" direct="false"/>
  <Role id="Group2Module" inverseId="Module2Group" source="Group" target="Module" maxCard="many" fieldName="modules" derived="false" direct="true"/>
  <Role id="Module2Group" inverseId="Group2Module" source="Module" target="Group" maxCard="many" fieldName="groups" derived="false" direct="false"/>
</DataModel>
